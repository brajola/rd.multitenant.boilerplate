{
    "name": "rd-pos-api",
    "private": true,
    "version": "1.0.0",
    "description": "Nest TypeScript starter repository",
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf dist",
        "doc": "npx @compodoc/compodoc -p tsconfig.json",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "NODE_OPTIONS=--require=elastic-apm-node nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "NODE_OPTIONS=--require=elastic-apm-node node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --verbose",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:clear": "jest --clearCache",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm:migrate": "npm run typeorm migration:generate -- -n",
        "typeorm:create": "npm run typeorm migration:create -- -n",
        "typeorm:up": "npm run typeorm -- migration:run",
        "typeorm:down": "npm run typeorm -- migration:revert",
        "pre-commit": "lint-staged"
    },
    "dependencies": {
        "@nestjs/common": "^8.4.3",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "8.4.3",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "1.0.1",
        "@nestjs/microservices": "8.4.3",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "8.4.3",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/typeorm": "8.0.3",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "date-fns": "2.28.0",
        "elastic-apm-node": "^3.43.0",
        "express": "4.17.3",
        "jsonwebtoken": "8.5.1",
        "jwt-decode": "3.1.2",
        "kafkajs": "1.15.0",
        "mysql2": "2.3.3",
        "passport": "^0.5.2",
        "passport-http-bearer": "1.0.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rosie": "2.1.0",
        "rxjs": "7.2.0",
        "swagger-ui-express": "4.3.0",
        "typeorm": "0.2.41",
        "typeorm-naming-strategies": "^4.1.0",
        "uuid": "8.3.2",
        "xlsx": "0.17.4"
    },
    "devDependencies": {
        "@compodoc/compodoc": "1.1.16",
        "@faker-js/faker": "6.0.0-alpha.5",
        "@nestjs/cli": "8.2.4",
        "@nestjs/schematics": "8.0.9",
        "@nestjs/testing": "8.4.3",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "4.17.13",
        "@types/faker": "5.5.9",
        "@types/jest": "27.0.3",
        "@types/node": "^16.11.11",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "5.5.0",
        "@typescript-eslint/parser": "5.5.0",
        "eslint": "8.3.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-airbnb-typescript": "16.0.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "^7.0.4",
        "jest": "27.3.1",
        "jest-ts-auto-mock": "2.1.0",
        "lint-staged": "12.1.2",
        "prettier": "2.4.1",
        "supertest": "6.1.6",
        "ts-auto-mock": "3.5.0",
        "ts-jest": "27.0.7",
        "ts-loader": "9.2.6",
        "ts-node": "10.4.0",
        "tsconfig-paths": "3.12.0",
        "ttypescript": "1.5.13",
        "typeorm-seeding": "1.6.1",
        "typescript": "4.3.5"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "compiler": "ttypescript"
            }
        },
        "rootDir": "src",
        "setupFiles": [
            "<rootDir>../test/jest-config.ts"
        ],
        "moduleNameMapper": {
            "@modules/(.+)": "<rootDir>/modules/$1",
            "@common/(.+)": "<rootDir>/common/$1",
            "@config/(.+)": "<rootDir>/config/$1",
            "@providers/(.+)": "<rootDir>/providers/$1"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            ".+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "modules/**/*.controller.(t|j)s",
            "modules/**/*.service.(t|j)s",
            "modules/**/*.repository.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
